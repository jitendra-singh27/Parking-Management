<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> - :AllCustomerDataElectricity : - </title>
    <link rel="stylesheet" href="/css/style.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">    
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
</head>
<body>
   <section id="header">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h2 id="l" >Electricity Customer Management Dashboard</h2>
                <a href="/projectmange"><button class="btn btn-danger" >Home</button></a>
            </div>
        </div>
    </div>
   </section> 
   <section id="mid">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <a href="/Elect/NewCustermerDetails"><button class="form-control btn btn-success mb-2 mt-2">New Customer</button></a>
                <table class="table table-hover mt-2">
                    <tr>
                        <th>S No.</th>
                        <th>Name </th>
                        <th>Address</th>
                        <th>Mobile No</th>
                        <th>K Number</th>
                        <th>Customer Type </th>
                        <th>Starting Reading</th>
                        <th>Previous Reading</th>
                        <th>Current Reading</th>
                        <th>Action</th>
                    </tr>
                    
                        <% record.forEach((result,key) => { %>
                            <tr>
                                <td><%=key+1%></td>
                                <td><%=result.name%></td>
                                <td><%= result.address %></td>
                                <td><%= result.mobile %></td>
                                <td><%= result.knumber %></td>
                                <td><%= result.mtype %></td>
                                <td><%= result.preading %></td>
                                <td><a href="/Elect/MeterReading/<%=result.id%>"><button class="btn btn-success">Reading</button></a></td>
                                <td><a href="/Elect/DeleteReading/<%=result.id%>"><button class="btn btn-danger"> Delete</button></a></td>
                            </tr>
                        <% }) %>
                      
                </table>
            </div>
        </div>
    </div>
   </section> 

   <!--Footer-->
   <section id="Header">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
             <h3>Footer</h3>
            </div>
        </div>
    </div>
   <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-kenU1KFdBIe4zVF0s0G1M5b4hcpxyD9F7jL+jjXkk+Q2h455rYXK/7HAuoJl+0I4" crossorigin="anonymous"></script>
</body>
</html>